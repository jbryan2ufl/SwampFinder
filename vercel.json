{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/server/(.*)", "dest": "server/$1" },
      { "src": "/(.*)", "dest": "src/$1" }
    ]
  }
  